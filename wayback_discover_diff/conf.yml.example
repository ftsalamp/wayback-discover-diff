simhash:
    size: 128
    expire_after: 86400

redis_uri: "redis://localhost:6379/1"

test_redis:
    port: 6379
    host: "localhost"
    db: 0

celery:
    backend_url: "redis://localhost:6379/2"
    broker_url: "redis://localhost:6379/3"
    task_default_queue: "wayback_discover_diff"
    task_soft_time_limit: 7200
    worker_max_tasks_per_child: 100

logfile:
    name: "log.txt"
    level: 20

threads: 8

snapshots:
    number_per_year: -1
    number_per_page: 600

cors:
    ['http://localhost:3000',
    'http://localhost:3001']
